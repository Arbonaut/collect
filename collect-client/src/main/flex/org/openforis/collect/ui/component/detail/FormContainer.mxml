<?xml version="1.0" encoding="utf-8"?>
<s:NavigatorContent xmlns:fx="http://ns.adobe.com/mxml/2009" 
					xmlns:s="library://ns.adobe.com/flex/spark" 
					xmlns:mx="library://ns.adobe.com/flex/mx" 
					width="100%" height="100%">
	
	<fx:Script>
		<![CDATA[
			private var _rootFormContent:RootEntityFormContainer;
			//TODO change to version object
			private var _version:Object;
			
			public function get rootFormContent():RootEntityFormContainer {
				return _rootFormContent;
			}
			
			public function set rootFormContent(value:RootEntityFormContainer):void {
				_rootFormContent = value;
			}
			
			public function get version():Object {
				return _version;
			}
			
			public function set version(value:Object):void {
				_version = value;
			}
			
			public function addEntityFormContainer(form:EntityFormContainer):void {
				this.viewStack.addElement(form)l
			}
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:ButtonBar id="buttonBar" dataProvider="{viewStack}" />
	<mx:ViewStack width="100%" height="100%%" id="viewStack">
		<!-- view stack for all forms-->
	</mx:ViewStack>
	
</s:NavigatorContent>
