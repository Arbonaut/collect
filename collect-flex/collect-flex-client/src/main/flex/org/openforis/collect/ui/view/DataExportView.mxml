		 xmlns:component="org.openforis.collect.ui.component.*"
		 width="100%" height="100%" 
		 >
	<s:layout>
		<s:HorizontalLayout />
	</s:layout>
	
	<fx:Script>
		<![CDATA[
			import org.openforis.collect.presenter.DataExportPresenter;
			
