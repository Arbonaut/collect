			override protected function initializationComplete():void {
				super.initializationComplete();
				_presenter = new DataExportPresenter(this);
			}
		]]>
	</fx:Script>
	
	<mx:Tree id="rootTree"
			 height="100%"
			 width="40%"
			 />
	
	<component:CheckBoxTree id="attributesTree"
							height="100%"
