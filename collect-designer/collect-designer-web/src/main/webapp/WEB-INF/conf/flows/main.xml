<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<var name="surveyName" class="java.lang.String" />

	<view-state id="selectSurvey">
		<on-render>
			<evaluate expression="surveyManager.getAll()"
				result="viewScope.surveys" />
		</on-render>
		<transition on="select" to="editSurvey">
<!-- 			<evaluate expression="searchCriteria.resetPage()" /> -->
		</transition>
		<transition on="newSurvey">
<!-- 			<evaluate expression="bookingService.cancelBooking(componentScope.booking)" /> -->
<!-- 			<render fragments="bookingsFragment" /> -->
		</transition>
	</view-state>

	<view-state id="editSurvey">
		<on-render>
			<evaluate expression="surveyManager.get(surveyName)"
				result="viewScope.survey" />
		</on-render>
		<transition on="cancelEdit" to="selectSurvey" />
	</view-state>

	<end-state id="finish" />

</flow>